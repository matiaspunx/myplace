{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(js|html)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=0"
      }]
    }]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
